@article{helmi_merger_2018,
	title = {The merger that led to the formation of the {Milky} {Way}'s inner stellar halo and thick disk},
	volume = {563},
	issn = {0028-0836, 1476-4687},
	url = {http://arxiv.org/abs/1806.06038},
	doi = {10.1038/s41586-018-0625-x},
	abstract = {The assembly process of our Galaxy can be retrieved using the motions and chemistry of individual stars. Chemo-dynamical studies of the nearby halo have long hinted at the presence of multiple components such as streams, clumps, duality and correlations between the stars' chemical abundances and orbital parameters. More recently, the analysis of two large stellar surveys have revealed the presence of a well-populated chemical elemental abundance sequence, of two distinct sequences in the colour-magnitude diagram, and of a prominent slightly retrograde kinematic structure all in the nearby halo, which may trace an important accretion event experienced by the Galaxy. Here report an analysis of the kinematics, chemistry, age and spatial distribution of stars in a relatively large volume around the Sun that are mainly linked to two major Galactic components, the thick disk and the stellar halo. We demonstrate that the inner halo is dominated by debris from an object which at infall was slightly more massive than the Small Magellanic Cloud, and which we refer to as Gaia-Enceladus. The stars originating in Gaia-Enceladus cover nearly the full sky, their motions reveal the presence of streams and slightly retrograde and elongated trajectories. Hundreds of RR Lyrae stars and thirteen globular clusters following a consistent age-metallicity relation can be associated to Gaia-Enceladus on the basis of their orbits. With an estimated 4:1 mass-ratio, the merger with Gaia-Enceladus must have led to the dynamical heating of the precursor of the Galactic thick disk and therefore contributed to the formation of this component approximately 10 Gyr ago. These findings are in line with simulations of galaxy formation, which predict that the inner stellar halo should be dominated by debris from just a few massive progenitors.},
	number = {7729},
	urldate = {2021-03-04},
	journal = {Nature},
	author = {Helmi, Amina and Babusiaux, Carine and Koppelman, Helmer H. and Massari, Davide and Veljanoski, Jovan and Brown, Anthony G. A.},
	month = nov,
	year = {2018},
	note = {arXiv: 1806.06038},
	keywords = {Astrophysics - Astrophysics of Galaxies},
	pages = {85--88},
	file = {arXiv.org Snapshot:/home/jbirky/Zotero/storage/ZWBEXB32/1806.html:text/html;helmi_et_al_2018_the_merger_that_led_to_the_formation_of_the_milky_way's_inner_stellar_halo_and.pdf:/home/jbirky/Dropbox/zotero/helmi_et_al_2018_the_merger_that_led_to_the_formation_of_the_milky_way's_inner_stellar_halo_and.pdf:application/pdf},
}

@article{belokurov_co-formation_2018,
	title = {Co-formation of the {Galactic} disc and the stellar halo},
	volume = {478},
	issn = {0035-8711, 1365-2966},
	url = {http://arxiv.org/abs/1802.03414},
	doi = {10.1093/mnras/sty982},
	abstract = {Using a large sample of Main Sequence stars with 7-D measurements supplied by Gaia and SDSS, we study the kinematic properties of the local (within {\textasciitilde}10 kpc from the Sun) stellar halo. We demonstrate that the halo's velocity ellipsoid evolves strongly with metallicity. At the low [Fe/H] end, the orbital anisotropy (the amount of motion in the radial direction compared to the tangential one) is mildly radial with 0.2{\textless}beta{\textless}0.4. However, for stars with [Fe/H]{\textgreater}-1.7 we measure extreme values of beta{\textasciitilde}0.9. Across the metallicity range considered, i.e. -3{\textless}[Fe/H]{\textless}-1, the stellar halo's spin is minimal, at the level of 20{\textless}v\_theta (km/s) {\textless}30. Using a suite of cosmological zoom-in simulations of halo formation, we deduce that the observed acute anisotropy is inconsistent with the continuous accretion of dwarf satellites. Instead, we argue, the stellar debris in the inner halo were deposited in a major accretion event by a satellite with Mvir{\textgreater}10{\textasciicircum}10 Msun around the epoch of the Galactic disc formation, i.e. between 8 and 11 Gyr ago. The radical halo anisotropy is the result of the dramatic radialisation of the massive progenitor's orbit, amplified by the action of the growing disc.},
	number = {1},
	urldate = {2021-03-05},
	journal = {Monthly Notices of the Royal Astronomical Society},
	author = {Belokurov, V. and Erkal, D. and Evans, N. W. and Koposov, S. E. and Deason, A. J.},
	month = jul,
	year = {2018},
	note = {arXiv: 1802.03414},
	keywords = {Astrophysics - Astrophysics of Galaxies},
	pages = {611--619},
	file = {arXiv.org Snapshot:/home/jbirky/Zotero/storage/87T86W2M/1802.html:text/html;belokurov_et_al_2018_co-formation_of_the_galactic_disc_and_the_stellar_halo.pdf:/home/jbirky/Dropbox/zotero/belokurov_et_al_2018_co-formation_of_the_galactic_disc_and_the_stellar_halo.pdf:application/pdf},
}

@article{helmi_streams_2020,
	title = {Streams, substructures and the early history of the {Milky} {Way}},
	volume = {58},
	issn = {0066-4146, 1545-4282},
	url = {http://arxiv.org/abs/2002.04340},
	doi = {10.1146/annurev-astro-032620-021917},
	abstract = {The advent of Gaia's 2nd data release in combination with large spectroscopic surveys are revolutionizing our understanding of the Galaxy. Thanks to these and the knowledge accumulated thus far, a more mature picture of the evolution of the early Milky Way is emerging: * Two of the traditional Galactic components, i.e. the stellar halo and the thick disk, appear to be intimately linked: stars with halo-like kinematics originate in similar proportions, from a "heated" (thick) disk and from debris from a system named Gaia-Enceladus. Gaia-Enceladus was the last big merger event experienced by the Milky Way and probably completed around 10 Gyr ago. The puffed-up stars now present in the halo as a consequence of the merger have thus exposed the existence of a disk component at z {\textasciitilde} 1.8. * The Helmi streams, Sequoia, and Thamnos are amongst the newly uncovered or better characterized merger events. Knowledge of their progenitor's properties, star formation and chemical histories is still incomplete. * Debris' from different objects often overlap in phase-space. A task for the next years will be to use spectroscopic surveys for chemical labelling and to disentangle events from one another using dimensions other than only phase-space, metallicity or [alpha/Fe]. * These surveys will also provide line-of-sight velocities missing for faint stars and more accurate distance determinations for distant objects. The resulting samples of stars will cover a much wider volume of the Galaxy allowing, for example, linking kinematic substructures in the inner halo to spatial overdensities in the outer halo. * All the results obtained so far are in-line with expectations of current cosmological models. Yet, tailored hydrodynamical simulations as well as "constrained" cosmological simulations are needed to push our knowledge of the assembly of the Milky Way back to the earliest times. [abridged]},
	number = {1},
	urldate = {2021-03-06},
	journal = {Annual Review of Astronomy and Astrophysics},
	author = {Helmi, Amina},
	month = aug,
	year = {2020},
	note = {arXiv: 2002.04340},
	keywords = {Astrophysics - Astrophysics of Galaxies},
	pages = {205--256},
	file = {arXiv.org Snapshot:/home/jbirky/Zotero/storage/V8D6JCAM/2002.html:text/html;helmi_2020_streams,_substructures_and_the_early_history_of_the_milky_way.pdf:/home/jbirky/Dropbox/zotero/helmi_2020_streams,_substructures_and_the_early_history_of_the_milky_way.pdf:application/pdf},
}

