	file = {arXiv.org Snapshot:/home/jbirky/Zotero/storage/DAQ69NQ9/1209.html:text/html;rodríguez_et_al_2012_spin-orbit_coupling_for_tidally_evolving_super-earths.pdf:/home/jbirky/Dropbox/zotero/rodríguez_et_al_2012_spin-orbit_coupling_for_tidally_evolving_super-earths.pdf:application/pdf},
