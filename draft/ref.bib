	keywords = {Astrophysics - Solar and Stellar Astrophysics, Astrophysics - Earth and Planetary Astrophysics, Astrophysics - Instrumentation and Methods for Astrophysics, Physics - Data Analysis, Statistics and Probability},
	file = {arXiv.org Snapshot:/home/jbirky/Zotero/storage/RVE683JL/2007.html:text/html;gordon_et_al_2020_a_fast,_2d_gaussian_process_method_based_on_celerite.pdf:/home/jbirky/Dropbox/zotero/gordon_et_al_2020_a_fast,_2d_gaussian_process_method_based_on_celerite.pdf:application/pdf},
