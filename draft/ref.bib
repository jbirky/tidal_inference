	pages = {891--913},
	file = {higson_et_al_2019_dynamic_nested_sampling.pdf:/home/jbirky/Dropbox/zotero/higson_et_al_2019_dynamic_nested_sampling.pdf:application/pdf},
}

@article{ribas_evolution_2005-1,
	title = {Evolution of the {Solar} {Activity} over {Time} and {Effects} on {Planetary} {Atmospheres}. {I}. {High}-{Energy} {Irradiances} (1-1700 Å)},
	volume = {622},
	issn = {0004-637X},
	url = {http://adsabs.harvard.edu/abs/2005ApJ...622..680R},
	doi = {10.1086/427977},
	abstract = {We report on the results of the Sun in Time multiwavelength program 
(X-rays to UV) of solar analogs with ages covering {\textasciitilde}0.1-7 Gyr. The chief
science goals are to study the solar magnetic dynamo and to determine
the radiative and magnetic properties of the Sun during its evolution
across the main sequence. The present paper focuses on the latter goal,
which has the ultimate purpose of providing the spectral irradiance
evolution of solar-type stars to be used in the study and modeling of
planetary atmospheres. The results from the Sun in Time program suggest
that the coronal X-ray-EUV emissions of the young main-sequence Sun were
{\textasciitilde}100-1000 times stronger than those of the present Sun. Similarly, the
transition region and chromospheric FUV-UV emissions of the young Sun
are expected to be 20-60 and 10-20 times stronger, respectively, than at
present. When we consider the integrated high-energy emission from 1 to
1200 Å, the resulting relationship indicates that about 2.5 Gyr
ago the solar high-energy flux was about 2.5 times the present value and
about 3.5 Gyr ago was about 6 times the present value (when life
supposedly arose on Earth). The strong radiation emissions inferred
should have had major influences on the thermal structure,
photochemistry, and photoionization of planetary atmospheres and have
played an important role in the development of primitive life in the
solar system. Some examples of the application of the Sun in Time
results on exoplanets and on early solar system planets are discussed.},
	urldate = {2021-01-17},
	journal = {The Astrophysical Journal},
	author = {Ribas, Ignasi and Guinan, Edward F. and Güdel, Manuel and Audard, Marc},
	month = mar,
	year = {2005},
	keywords = {Stars: Late-Type, Stars: Activity, Stars: Coronae, Stars: Chromospheres, Sun: Evolution, Sun: Solar-terrestrial Relations},
	pages = {680--694},
	file = {ribas_et_al_2005_evolution_of_the_solar_activity_over_time_and_effects_on_planetary_atmospheres.pdf:/home/jbirky/Dropbox/zotero/ribas_et_al_2005_evolution_of_the_solar_activity_over_time_and_effects_on_planetary_atmospheres.pdf:application/pdf},
}

@article{chen_neural_2019,
	title = {Neural {Ordinary} {Differential} {Equations}},
	url = {http://arxiv.org/abs/1806.07366},
	abstract = {We introduce a new family of deep neural network models. Instead of specifying a discrete sequence of hidden layers, we parameterize the derivative of the hidden state using a neural network. The output of the network is computed using a black-box differential equation solver. These continuous-depth models have constant memory cost, adapt their evaluation strategy to each input, and can explicitly trade numerical precision for speed. We demonstrate these properties in continuous-depth residual networks and continuous-time latent variable models. We also construct continuous normalizing flows, a generative model that can train by maximum likelihood, without partitioning or ordering the data dimensions. For training, we show how to scalably backpropagate through any ODE solver, without access to its internal operations. This allows end-to-end training of ODEs within larger models.},
	urldate = {2021-01-31},
	journal = {arXiv:1806.07366 [cs, stat]},
	author = {Chen, Ricky T. Q. and Rubanova, Yulia and Bettencourt, Jesse and Duvenaud, David},
	month = dec,
	year = {2019},
	note = {arXiv: 1806.07366},
	keywords = {Statistics - Machine Learning, Computer Science - Machine Learning, Computer Science - Artificial Intelligence},
	file = {arXiv.org Snapshot:/home/jbirky/Zotero/storage/6CIDRBMR/1806.html:text/html;chen_et_al_2019_neural_ordinary_differential_equations.pdf:/home/jbirky/Dropbox/zotero/chen_et_al_2019_neural_ordinary_differential_equations.pdf:application/pdf},
}

@article{luger_habitable_2015,
	title = {Habitable {Evaporated} {Cores}: {Transforming} {Mini}-{Neptunes} into {Super}-{Earths} in the {Habitable} {Zones} of {M} {Dwarfs}},
	volume = {15},
	issn = {1531-1074, 1557-8070},
	shorttitle = {Habitable {Evaporated} {Cores}},
	url = {http://arxiv.org/abs/1501.06572},
	doi = {10.1089/ast.2014.1215},
	abstract = {We show that photoevaporation of small gaseous exoplanets ("mini-Neptunes") in the habitable zones of M dwarfs can remove several Earth masses of hydrogen and helium from these planets and transform them into potentially habitable worlds. We couple X-ray/extreme ultraviolet (XUV)-driven escape, thermal evolution, tidal evolution and orbital migration to explore the types of systems that may harbor such "habitable evaporated cores" (HECs). We find that HECs are most likely to form from planets with \${\textbackslash}sim 1 M\_{\textbackslash}oplus\$ solid cores with up to about 50\% H/He by mass, though whether or not a given mini-Neptune forms a HEC is highly dependent on the early XUV evolution of the host star. As terrestrial planet formation around M dwarfs by accumulation of local material is likely to form planets that are small and dry, evaporation of small migrating mini-Neptunes could be one of the dominant formation mechanisms for volatile-rich Earths around these stars.},
	number = {1},
	urldate = {2021-02-01},
	journal = {Astrobiology},
	author = {Luger, Rodrigo and Barnes, Rory and Lopez, Eric and Fortney, Jonathan and Jackson, Brian and Meadows, Victoria},
	month = jan,
	year = {2015},
	note = {arXiv: 1501.06572},
	keywords = {Astrophysics - Earth and Planetary Astrophysics},
	pages = {57--88},
	file = {arXiv.org Snapshot:/home/jbirky/Zotero/storage/NWFCEQ3R/1501.html:text/html;luger_et_al_2015_habitable_evaporated_cores.pdf:/home/jbirky/Dropbox/zotero/luger_et_al_2015_habitable_evaporated_cores.pdf:application/pdf},
