	pages = {8},
	file = {arXiv Fulltext PDF:/home/jbirky/Zotero/storage/LMN9L395/Tchernyshyov and Peek - 2016 - Kinetic Tomography I A Method for Mapping the Mil.pdf:application/pdf;arXiv.org Snapshot:/home/jbirky/Zotero/storage/LB2KVH5W/1611.html:text/html},
}

@article{tchernyshyov_kinetic_2018,
