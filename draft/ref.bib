	file = {arXiv.org Snapshot:/home/jbirky/Zotero/storage/N6FSHJ52/2110.html:text/html;özdarcan_2021_two_newly_identified_eclipsing_binaries_in_open_cluster_ngc_3532.pdf:/home/jbirky/Dropbox/zotero/özdarcan_2021_two_newly_identified_eclipsing_binaries_in_open_cluster_ngc_3532.pdf:application/pdf},
