@article{burger_conservation_2020,
	title = {Conservation of radial actions in time-dependent spherical potentials},
	url = {http://arxiv.org/abs/2012.00737},
	abstract = {In slowly evolving spherical potentials, \${\textbackslash}Phi(r,t)\$, radial actions are typically assumed to remain constant. Here, we construct dynamical invariants that allow us to derive the generic evolution of radial actions in spherical central potentials with an arbitrary time dependence. We show that the radial actions oscillate around a constant value with an amplitude \${\textbackslash}Delta J\_r {\textbackslash}propto {\textbackslash}dot\{{\textbackslash}Phi\}/{\textbackslash}Phi{\textbackslash},P(E,L)\$, where \$P\$ is the radial period of an orbit with energy \$E\$ and angular momentum \$L\$. Using this result, we develop a diffusion theory that describes the evolution of the radial action distribution of ensembles of tracer particles orbiting in time-dependent spherical potentials. Tests against restricted \$N\$-body simulations in a varying Keplerian potential indicate that our linear theory is accurate in regions of phase-space in which the diffusion coefficient \${\textbackslash}tilde\{D\}(J\_r) {\textless} 0.01{\textbackslash},J\_r{\textasciicircum}2\$. For illustration, we show that i) in Milky Way (MW) dark matter haloes with a median mass accretion history only \${\textbackslash}sim 40\$ per cent of halo particles evolve within the linear regime since the redshift of maximum accretion, which calls against using radial actions as `adiabatic invariants' when attempting to theoretically predict the density profiles of DM haloes, or construct models for tidal streams in action-angle space. ii) In contrast, the cusp-core transformation of dwarf haloes in self-interacting dark matter (SIDM) models (with \${\textbackslash}sigma/m\_{\textbackslash}chi = 1{\textbackslash},\{{\textbackslash}rm cm{\textasciicircum}2g{\textasciicircum}\{-1\}\}\$) leads to haloes in which more than \${\textbackslash}sim 97\$ per cent of DM particles exhibit a linear behaviour of radial actions. We conclude that the radial action distribution of stellar tracers in SIDM haloes is hardly affected by self-collisions in the dark sector.},
	urldate = {2020-12-02},
	journal = {arXiv:2012.00737 [astro-ph]},
	author = {Burger, Jan D. and Peñarrubia, Jorge and Zavala, Jesús},
	month = dec,
	year = {2020},
	note = {arXiv: 2012.00737},
	keywords = {Astrophysics - Astrophysics of Galaxies},
	file = {arXiv.org Snapshot:/home/jbirky/Zotero/storage/XQG7S57H/2012.html:text/html;burger_et_al_2020_conservation_of_radial_actions_in_time-dependent_spherical_potentials.pdf:/home/jbirky/Dropbox/zotero/burger_et_al_2020_conservation_of_radial_actions_in_time-dependent_spherical_potentials.pdf:application/pdf},
