@article{efroimsky_tidal_2009,
	title = {Tidal torques. {A} critical review of some techniques},
	volume = {104},
	issn = {0923-2958, 1572-9478},
	url = {http://arxiv.org/abs/0803.3299},
	doi = {10.1007/s10569-009-9204-7},
	abstract = {We point out that the MacDonald formula for body-tide torques is valid only in the zeroth order of e/Q, while its time-average is valid in the first order. So the formula cannot be used for analysis in higher orders of e/Q. This necessitates corrections in the theory of tidal despinning and libration damping. We prove that when the inclination is low and phase lags are linear in frequency, the Kaula series is equivalent to a corrected version of the MacDonald method. The correction to MacDonald's approach would be to set the phase lag of the integral bulge proportional to the instantaneous frequency. The equivalence of descriptions gets violated by a nonlinear frequency-dependence of the lag. We explain that both the MacDonald- and Darwin-torque-based derivations of the popular formula for the tidal despinning rate are limited to low inclinations and to the phase lags being linear in frequency. The Darwin-torque-based derivation, though, is general enough to accommodate both a finite inclination and the actual rheology. Although rheologies with Q scaling as the frequency to a positive power make the torque diverge at a zero frequency, this reveals not the impossible nature of the rheology, but a flaw in mathematics, i.e., a common misassumption that damping merely provides lags to the terms of the Fourier series for the tidal potential. A hydrodynamical treatment (Darwin 1879) had demonstrated that the magnitudes of the terms, too, get changed. Reinstating of this detail tames the infinities and rehabilitates the "impossible" scaling law (which happens to be the actual law the terrestrial planets obey at low frequencies).},
	number = {3},
	urldate = {2021-12-30},
	journal = {Celestial Mechanics and Dynamical Astronomy},
	author = {Efroimsky, Michael and Williams, James G.},
	month = jul,
	year = {2009},
	note = {arXiv: 0803.3299},
	keywords = {Astrophysics, Physics - Classical Physics, Physics - Geophysics, Physics - History and Philosophy of Physics},
	pages = {257--289},
	file = {arXiv.org Snapshot:/home/jbirky/Zotero/storage/PM694FFQ/0803.html:text/html;efroimsky_williams_2009_tidal_torques.pdf:/home/jbirky/Dropbox/zotero/efroimsky_williams_2009_tidal_torques.pdf:application/pdf},
