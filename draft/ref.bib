@article{spina_how_2020,
	title = {How magnetic activity alters what we learn from stellar spectra},
	url = {http://arxiv.org/abs/2004.09771},
	abstract = {Magnetic fields and stellar spots can alter the equivalent widths of absorption lines in stellar spectra, varying during the activity cycle. This also influences the information that we derive through spectroscopic analysis. In this study we analyse high-resolution spectra of 211 Sun-like stars observed at different phases of their activity cycles, in order to investigate how stellar activity affects the spectroscopic determination of stellar parameters and chemical abundances. We observe that equivalent widths of lines can increase as a function of the activity index log R\${\textasciicircum}{\textbackslash}prime\_\{{\textbackslash}rm HK\}\$ during the stellar cycle, which also produces an artificial growth of the stellar microturbulence and a decrease in effective temperature and metallicity. This effect is visible for stars with activity indexes log R\${\textasciicircum}{\textbackslash}prime\_\{{\textbackslash}rm HK\}\$\${\textbackslash}geq\$\$-\$5.0 (i.e., younger than 4-5 Gyr) and it is more significant at higher activity levels. These results have fundamental implications on several topics in astrophysics that are discussed in the paper, including stellar nucleosynthesis, chemical tagging, the study of Galactic chemical evolution, chemically anomalous stars, the structure of the Milky Way disk, stellar formation rates, photoevaporation of circumstellar disks, and planet hunting.},
	urldate = {2020-04-22},
	journal = {arXiv:2004.09771 [astro-ph]},
	author = {Spina, Lorenzo and Nordlander, Thomas and Casey, Andrew R. and Bedell, Megan and D'Orazi, Valentina and Meléndez, Jorge and Karakas, Amanda I. and Desidera, Silvano and Baratella, Martina and Galarza, Jhon J. Yana and Casali, Giada},
	month = apr,
	year = {2020},
	note = {arXiv: 2004.09771},
	keywords = {Astrophysics - Solar and Stellar Astrophysics, Astrophysics - Instrumentation and Methods for Astrophysics, 85-02, J.2},
	file = {arXiv.org Snapshot:/home/jbirky/Zotero/storage/JSBCK4QJ/2004.html:text/html;spina_et_al_2020_how_magnetic_activity_alters_what_we_learn_from_stellar_spectra.pdf:/home/jbirky/Dropbox/zotero/spina_et_al_2020_how_magnetic_activity_alters_what_we_learn_from_stellar_spectra.pdf:application/pdf},
