	file = {arXiv.org Snapshot:/home/jbirky/Zotero/storage/A95LLAUX/2003.html:text/html;ruiz-lara_et_al_2020_the_recurrent_impact_of_the_sagittarius_dwarf_on_the_milky_way_star_formation.pdf:/home/jbirky/Dropbox/zotero/ruiz-lara_et_al_2020_the_recurrent_impact_of_the_sagittarius_dwarf_on_the_milky_way_star_formation.pdf:application/pdf},
