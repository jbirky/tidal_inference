	journal = {Astronomy \& Astrophysics},
	author = {Cantat-Gaudin, T. and Anders, F. and Castro-Ginard, A. and Jordi, C. and Romero-Gomez, M. and Soubiran, C. and Casamiquela, L. and Tarricq, Y. and Moitinho, A. and Vallenari, A. and Bragaglia, A. and Krone-Martins, A. and Kounkel, M.},
