@article{david_k2_2015,
	title = {K2 {Discovery} of {Young} {Eclipsing} {Binaries} in {Upper} {Scorpius}: {Direct} {Mass} and {Radius} {Determinations} for the {Lowest} {Mass} {Stars} and {Initial} {Characterization} of an {Eclipsing} {Brown} {Dwarf} {Binary}},
	volume = {816},
	issn = {1538-4357},
	shorttitle = {K2 {Discovery} of {Young} {Eclipsing} {Binaries} in {Upper} {Scorpius}},
	url = {http://arxiv.org/abs/1510.08087},
	doi = {10.3847/0004-637X/816/1/21},
	abstract = {We report the discovery of three low-mass double-lined eclipsing binaries in the pre-main sequence Upper Scorpius association, revealed by \$K2\$ photometric monitoring of the region over \${\textbackslash}sim\$ 78 days. The orbital periods of all three systems are \${\textless}\$5 days. We use the \$K2\$ photometry plus multiple Keck/HIRES radial velocities and spectroscopic flux ratios to determine fundamental stellar parameters for both the primary and secondary components of each system, along with the orbital parameters. We present tentative evidence that EPIC 203868608 is a hierarchical triple system comprised of an eclipsing pair of \${\textbackslash}sim\$25 \$M\_{\textbackslash}mathrm\{Jup\}\$ brown dwarfs with a wide M-type companion. If confirmed, it would constitute only the second double-lined eclipsing brown dwarf binary system discovered to date. The double-lined system EPIC 203710387 is composed of nearly identical M4.5-M5 stars with fundamentally determined masses and radii measured to better than 3\% precision (\$M\_1=0.1183{\textbackslash}pm0.0028 M\_{\textbackslash}odot\$, \$M\_2=0.1076{\textbackslash}pm0.0031 M\_{\textbackslash}odot\$ and \$R\_1=0.417{\textbackslash}pm0.010 R\_{\textbackslash}odot\$, \$R\_2=0.450{\textbackslash}pm0.012 R\_{\textbackslash}odot\$) from combination of the light curve and radial velocity time series. These stars have the lowest masses of any stellar mass double-lined eclipsing binary to date. Finally, EPIC 203476597 is a compact single-lined system with a G8-KO primary and a likely mid-K secondary whose line are revealed in spectral ratios. Continued measurement of radial velocities and spectroscopic flux ratios will better constrain fundamental parameters and should elevate the objects to benchmark status. We also present revised parameters for the double-lined eclipsing binary UScoCTIO 5 (\$M\_1=0.3336{\textbackslash}pm0.0022 M\_{\textbackslash}odot\$, \$M\_2=0.3200{\textbackslash}pm0.0022 M\_{\textbackslash}odot\$ and \$R\_1=0.862{\textbackslash}pm0.012\$, \$R\_2=0.852{\textbackslash}pm0.013 R\_{\textbackslash}odot\$). We discuss the implications of our results on these \${\textbackslash}sim\$0.1-1.5 \$M\_{\textbackslash}odot\$ stars for pre-main-sequence evolutionary models.},
	number = {1},
	urldate = {2020-09-10},
	journal = {The Astrophysical Journal},
	author = {David, Trevor J. and Hillenbrand, Lynne A. and Cody, Ann Marie and Carpenter, John M. and Howard, Andrew W.},
	month = dec,
	year = {2015},
	note = {arXiv: 1510.08087},
	keywords = {Astrophysics - Solar and Stellar Astrophysics},
	pages = {21},
	file = {arXiv.org Snapshot:/home/jbirky/Zotero/storage/IHGGV8QL/1510.html:text/html;david_et_al_2015_k2_discovery_of_young_eclipsing_binaries_in_upper_scorpius.pdf:/home/jbirky/Dropbox/zotero/david_et_al_2015_k2_discovery_of_young_eclipsing_binaries_in_upper_scorpius.pdf:application/pdf},
}

@article{david_new_2016,
	title = {New {Pleiades} {Eclipsing} {Binaries} and a {Hyades} {Transiting} {System} {Identified} by {K2}},
