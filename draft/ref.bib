@article{spina_galah_2020,
	title = {The {GALAH} survey: tracing the {Galactic} disk with {Open} {Clusters}},
	shorttitle = {The {GALAH} survey},
	url = {http://arxiv.org/abs/2011.02533},
	abstract = {Open clusters are unique tracers of the history of our own Galaxy's disk. According to our membership analysis based on {\textbackslash}textit\{Gaia\} astrometry, out of the 226 potential clusters falling in the footprint of GALAH or APOGEE, we find that 205 have secure members that were observed by at least one of the survey. Furthermore, members of 134 clusters have high-quality spectroscopic data that we use to determine their chemical composition. We leverage this information to study the chemical distribution throughout the Galactic disk of 21 elements, from C to Eu, and including Fe. The radial metallicity gradient obtained from our analysis is \$-\$0.076\${\textbackslash}pm\$0.009 dex kpc\${\textasciicircum}\{-1\}\$, which is in agreement with previous works based on smaller samples. Furthermore, the gradient in the [Fe/H] - guiding radius (r\$\_\{{\textbackslash}rm guid\}\$) plane is \$-\$0.073\${\textbackslash}pm\$0.008 dex kpc\${\textasciicircum}\{-1\}\$. We also reveal trends in the [X/Fe] - r\$\_\{{\textbackslash}rm guid\}\$ - age space, which are important to understand production rates of different elements as a function of space and time. Finally, we observe subtle - but significant - differences on how field stars and open clusters trace the distribution of chemical elements throughout the Galactic disk. As it is discussed in the paper, these differences are probably due to the physical selection effect imposed by our Galaxy, which preserves the clusters living far from its gravitational potential perturbations (e.g., spirals, bars, molecular clouds), while it destroys all the others.},
	urldate = {2020-11-10},
	journal = {arXiv:2011.02533 [astro-ph]},
	author = {Spina, Lorenzo and Ting, Yuan-Sen and De Silva, Gayandhi M. and Frankel, Neige and Sharma, Sanjib and Cantat-Gaudin, Tristan and Joyce, Meridith and Stello, Dennis and Karakas, Amanda I. and Asplund, Martin B. and Nordlander, Thomas and Casagrande, Luca and D'Orazi, Valentina and Casey, Andrew R. and Cottrell, Peter and Tepper-García, Thor and Baratella, Martina and Kos, Janez and Čotar, Klemen and Bland-Hawthorn, Joss and Buder, Sven and Freeman, Ken C. and Hayden, Michael R. and Lewis, Geraint F. and Lin, Jane and Lind, Karin and Martell, Sarah L. and Schlesinger, Katharine J. and Simpson, Jeffrey D. and Zucker, Daniel B. and Zwitter, Tomaž},
