	file = {arXiv.org Snapshot:/home/jbirky/Zotero/storage/NFPVVBS3/1810.html:text/html;luger_et_al_2019_starry.pdf:/home/jbirky/Dropbox/zotero/luger_et_al_2019_starry.pdf:application/pdf},
